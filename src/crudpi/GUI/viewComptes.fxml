<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane"  prefHeight="494.0" prefWidth="897.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="crudpi.GUI.ViewComptesController">
   <children>
      <TableView fx:id="tableview" layoutX="-1.0" layoutY="48.0" prefHeight="351.0" prefWidth="793.0">
        <columns>
          <TableColumn fx:id="col1" prefWidth="75.0" text="id" />
          <TableColumn fx:id="col2" prefWidth="75.0" text="nom" />
            <TableColumn fx:id="col3" prefWidth="75.0" text="prenom" />
            <TableColumn fx:id="col4" prefWidth="75.0" text="email" />
            <TableColumn fx:id="col5" prefWidth="131.0" text="num telefone" />
            <TableColumn fx:id="col6" prefWidth="107.0" text="solde" />
            <TableColumn fx:id="delete" prefWidth="75.0" text="delete" />
        </columns>
      </TableView>
      <Button fx:id="addBtn" layoutX="28.0" layoutY="14.0" mnemonicParsing="false" onAction="#showAdd" prefHeight="31.0" prefWidth="141.0" text="Add Compte" />
      <Button fx:id="pdf" layoutX="243.0" layoutY="14.0" mnemonicParsing="false" onAction="#pdf" text="pdf" />
   </children>
</AnchorPane>
